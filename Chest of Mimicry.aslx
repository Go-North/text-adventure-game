  <object name="Alley">
    <inherit name="editor_room" />
    <objectslistprefix>I could see</objectslistprefix>
    <description type="script"><![CDATA[
      if (player.turns < 200) {
        msg ("<b>Alley</b>: I arrive at the crime scene, stepping past the police tape I see a mangled body lying in the Alley.")
      }
      else {
        msg ("<b>Alley</b>: Looks neater wihtout a dead body lying in the streets")
      }
    ]]></description>
    <firstenter type="script">
      UnlockExit (Office School)
      UnlockExit (Office Marketplace)
      UnlockExit (Downtown Rookters)
      UnlockExit (Downtown Apartments)
    </firstenter>
    <enter type="script">
